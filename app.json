{
  "name": "twomnistrate",
  "description": "sinatra example implementation of omniauth twitter with CenturyLink Cloud",
  "repository": "https://github.com/pymander/twomnistrate",
  "keywords": ["ruby", "sinatra", "orchestrate", "omniauth", "twitter"],
  "website": "http://twomnistrate.herokuapp.com",
  "env": {
    "CONSUMER_KEY": {
      "description": "twitter public key to generate auth url",
      "required": true
    },
    "CONSUMER_SECRET": {
      "description": "twitter secret api key",
      "required": true
    },
    "MONGO_HOST": {
      "description": "MongoDB host",
      "required": true
    },
    "MONGO_DB": {
      "description": "MongoDB database name",
      "required": true
    }
  }
}
